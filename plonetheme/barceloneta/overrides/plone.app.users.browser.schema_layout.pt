<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="here/prefs_main_template/macros/master"
      i18n:domain="plone">

<body>

<div metal:fill-slot="prefs_configlet_main">

  <a id="setup-link" class="link-parent"
     tal:attributes="href string:$portal_url/@@overview-controlpanel"
     i18n:translate="">
      Site Setup
  </a>

  <h1 class="documentFirstHeading" i18n:translate="">Edit Member Form Fields</h1>

  <div metal:use-macro="context/global_statusmessage/macros/portal_message">
    Portal status message
  </div>

  <div id="content-core">

        <ul class="nav nav-tabs">
            <li class="nav-item">
              <a class="nav-link"
                href="${portal_url}/@@usergroup-userprefs"
                i18n:translate="label_users">Users</a>
            </li>
            <li class="nav-item">
              <a href="${portal_url}/@@usergroup-groupprefs" class="nav-link"
                i18n:translate="label_groups">Groups</a>
            </li>
            <li class="nav-item">
              <a href="${portal_url}/@@usergroup-controlpanel" class="nav-link"
                i18n:translate="label_usergroup_settings">Settings</a>
            </li>
            <li class="nav-item">
              <a href="${portal_url}/@@member-fields" class="nav-link active"
                i18n:translate="label_member_fields">Member fields</a>
            </li>
        </ul>

      <tal:block tal:replace="structure view/contents|view/render"/>

  </div>

</div>

</body>
</html>
